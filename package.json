{
  "name": "@typhonjs-svelte/lib",
  "version": "0.0.0",
  "description": "Provides a common library for TyphonJS Svelte applications.",
  "type": "module",
  "author": "Michael Leahy <support@typhonjs.io> (https://github.com/typhonrt)",
  "contributors": [
    "Michael Leahy <support@typhonjs.io> (https://github.com/typhonrt)"
  ],
  "sideEffects": false,
  "imports": {
    "#runtime/*": "@typhonjs-svelte/runtime-base/*",
    "#svelte-lib/*": "@typhonjs-svelte/lib/*",
    "#svelte/*": "svelte/*",
    "#svelte": "svelte"
  },
  "exports": {
    "./action/dom": {
      "types": "./_dist/action/dom/index.d.ts",
      "import": "./_dist/action/dom/index.js"
    },
    "./animate": {
      "types": "./_dist/animate/index.d.ts",
      "import": "./_dist/animate/index.js"
    },
    "./animate/action": {
      "types": "./_dist/animate/action/index.d.ts",
      "import": "./_dist/animate/action/index.js"
    },
    "./store/derived": {
      "types": "./_dist/store/derived/index.d.ts",
      "import": "./_dist/store/derived/index.js"
    },
    "./store/dom": {
      "types": "./_dist/store/dom/index.d.ts",
      "import": "./_dist/store/dom/index.js"
    },
    "./store/position": {
      "types": "./_dist/store/position/index.d.ts",
      "import": "./_dist/store/position/index.js"
    },
    "./store/storage/web/plugin": {
      "types": "./_dist/store/storage/web/plugin/index.d.ts",
      "import": "./_dist/store/storage/web/plugin/index.js"
    },
    "./transition": {
      "types": "./_dist/transition/index.d.ts",
      "import": "./_dist/transition/index.js"
    },
    "./util": {
      "types": "./_dist/util/index.d.ts",
      "import": "./_dist/util/index.js"
    },
    "./package.json": "./package.json"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15",
    "@typhonjs-build-test/esm-d-ts": "^0.0.26",
    "@typhonjs-build-test/rollup-plugin-pkg-imports": "^0.0.2",
    "@typhonjs-config/eslint-config": "^0.6.3",
    "@typhonjs-svelte/runtime-base": "github:typhonjs-svelte/runtime-base",
    "eslint": "^8",
    "rollup": "^3",
    "svelte-writable-derived": "^3.1.0",
    "upath": "^2.0.1"
  },
  "peerDependencies": {
    "svelte": ">=4.x.x"
  },
  "engines": {
    "node": ">=14.18"
  },
  "homepage": "https://github.com/typhonjs-svelte/lib#readme",
  "keywords": [
    "TyphonJS",
    "Svelte",
    "library"
  ],
  "scripts": {
    "build": "node rollupAll.js",
    "eslint": "eslint ."
  },
  "files": [
    "/_dist",
    "/src"
  ],
  "directories": {},
  "license": "MPL-2.0",
  "repository": "github:typhonjs-svelte/lib",
  "bugs": "https://github.com/typhonjs-svelte/lib/issues"
}
